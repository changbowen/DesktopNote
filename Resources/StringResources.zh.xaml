    <sys:String x:Key="menu_auto_dock">屏幕边缘停靠</sys:String>
    <sys:String x:Key="menu_auto_start">随系统启动</sys:String>
    <sys:String x:Key="menu_exit">退出</sys:String>
    <sys:String x:Key="menu_close">关闭笔记</sys:String>
